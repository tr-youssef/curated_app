{
  "expo": {
    "name": "curated_app",
    "slug": "curated_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "pk.eyJ1IjoiZGppbnBhcmsxIiwiYSI6ImNsczZmMHR5YTBucWMyam54cDBwYnh2MDIifQ.05SO4raKDtndy7LISP2axQ"
        },
        "expo-location",
        {
          "locationWhenInUsePermission": "Show current location on map."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "iosbundle"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.djinpark1.curated_app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
